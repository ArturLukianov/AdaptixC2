{
  "extender_type": "listener",
  "extender_file": "listener_cog_stego_http.so",

  "listener_name": "CogStegoHTTP",
  "listener_type": "external",
  "protocol": "tcp",

  "ui": {
    "layout": "glayout",
    "elements": [
      {
        "type": "label",
        "text": "Host & port (Bind):",
        "position": [ 0, 0, 1, 1 ]
      },
      {
        "type": "input",
        "id": "host_bind",
        "editable": "false",
        "position": [ 0, 1, 1, 2 ]
      },
      {
        "type": "spinbox",
        "id": "port_bind",
        "editable": "false",
        "min": 1,
        "max": 65535,
        "value": 4444,
        "position": [0, 3, 1, 1]
      },
      {
        "type": "label",
        "text": "Callback addresses:",
        "position": [ 1, 0, 1, 1 ]
      },
      {
        "type": "textedit",
        "id": "callback_addresses",
        "placeholder": "192.168.1.1:4444\nserver2.com:5555",
        "position": [ 1, 1, 1, 3 ]
      },
      {
        "type": "label",
        "text": "Timeout (sec):",
        "position": [ 2, 0, 1, 1 ]
      },
      {
        "type": "spinbox",
        "id": "timeout",
        "min": 1,
        "max": 1000000,
        "value": 10,
        "position": [2, 1, 1, 3]
      }
    ]
  }
}